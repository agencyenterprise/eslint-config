{
  "name": "@ae-studio/eslint-config",
  "version": "3.0.0",
  "description": "ESLint config for TS and JS following the AE Studio code style.",
  "type": "module",
  "exports": {
    "./node": {
      "types": "./src/node.d.ts",
      "default": "./src/node.mjs"
    },
    "./react": {
      "types": "./src/react.d.ts",
      "default": "./src/react.mjs"
    },
    "./tailwind": {
      "types": "./src/tailwind.d.ts",
      "default": "./src/tailwind.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/agencyenterprise/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "config",
    "aestudio"
  ],
  "author": "AE Studio",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.11.0",
    "typescript-eslint": "^8.11.0"
  },
  "devDependencies": {
    "eslint": "^9.13.0"
  }
}
